local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local window = Rayfield:CreateWindow({
    Name = "barzil - Alpha Hax",
    LoadingTitle = "Loading Assets..",
    LoadingSubtitle = "Created By Zale",
    ConfigurationSaving = {
        Enabled = false,
        FileName = "Sirius"
    },
    KeySystem = true,
    KeySettings = {
        Title = "Zale's Key System",
        Subtitle = "Enter Your Key",
        Note = "Rate The Features",
        FileName = "Sirius",
        SaveKey = false,
        Key = "2025"
    }
})

local fanTab = window:CreateTab("Fan")
local badgeSection = fanTab:CreateSection("Badge")

fanTab:CreateButton({
    Name = "Auto Get Fan",
    Callback = function()
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local Events = ReplicatedStorage:WaitForChild("RemoteEvents")

        Events.SuitUpClown:FireServer()
        wait(0.1)
        Events.KeyQuest:FireServer()
        wait(0.1)
        Events.KeyAcquired:FireServer()
        wait(0.1)
        Events.GOHOME:FireServer()
        wait(0.1)
        Events.KeyBadgeReward:FireServer()
    end,
    SectionParent = badgeSection
})
